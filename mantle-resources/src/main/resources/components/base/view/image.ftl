<@thumborImg class=extractClasses(manifest.component.referenceIdChain) + ' ' + extractClasses(model.class!'')
             url=model.url!''
             img=model.imageObjectId!(model.image!'')
             width=model.width!model.image.width
             height=model.height!(model.image.height!0)
             alt=model.alt!(model.image.alt!'')
             lazyload=model.lazyLoad!false
             srcset=model.srcset!{}
             originalWidth=(model.image.width)!0
             originalHeight=(model.image.height)!0
             placeholder=model.placeholder!false
             customPlaceholder=model.customPlaceholder!''
             fitInStyle=model.fitInStyle!''
/>